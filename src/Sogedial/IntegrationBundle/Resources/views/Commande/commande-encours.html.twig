{% extends "SogedialIntegrationBundle:Layout:layout-infinite.html.twig" %}
{% block body %}
    <section>
        <div class="content orders-history">
        	<div class="breadcrumb-trail-container">
             {{ wo_render_breadcrumbs({ listId: 'wo-breadcrumbs-dashboard-icon' }) }}
            </div>
            <div class="container">
                <div class="space-between-container space-between-container-very-small">
                    <div class="page-title">Historique des commandes</div>
                </div>
                <div class="scrollable-content scrollable-content-order-list">
                    {% include "SogedialIntegrationBundle:Commande:tableau-commandes.html.twig"  with { 'societe': societe, "commandeEncours" : orders } %}
                    <div id="loadingDiv" style="display: none">
                        <div class="loader-commande-point-com">
                            {% include "SogedialIntegrationBundle:Utils:loaderCommandePointCom.html.twig"  with {'svg_id':1} %}
                        </div>
                    </div>
                    <div id="endLoadingDiv" style="display: none">
                        <p> Fin de la liste </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% block javascripts %}
    <script type="text/javascript">
    $(function () {
        $(".sticky-header-table").stickyTableHeaders();
    });
    </script>
{% endblock %}
{% endblock %}
